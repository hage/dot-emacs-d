# coding: utf-8
require 'rubygems'
require 'minitest/autorun'

# [documents](https://www.rubydoc.info/gems/minitest)

class $0Test < MiniTest::Test
  def setup
    @foo = Foo.new
  end

  # teardown はあまり使わない
  def teardown
    @foo = nil
  end

  def test_foo
    assert_equal "foo", @foo.foo
  end
end
